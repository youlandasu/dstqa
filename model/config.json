{
    "dataset_reader": {
        "type": "dstqa",
        "domain_slot_list_path": "./ontology/domain_slot_list_nosp.txt",
        "lazy": false,
        "token_indexers": {
            "token_characters": {
                "type": "characters",
                "character_tokenizer": {
                    "byte_encoding": "utf-8",
                    "end_tokens": [
                        260
                    ],
                    "start_tokens": [
                        259
                    ]
                },
                "min_padding_length": 5
            }
        },
        "tokenizer": {
            "type": "word",
            "word_splitter": {
                "type": "spacy",
                "ner": false,
                "pos_tags": false
            }
        }
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 36,
        "max_instances_in_memory": 1000,
        "sorting_keys": [
            [
                "dialogs",
                "num_tokens"
            ]
        ],
        "track_epoch": true
    },
    "model": {
        "type": "dstqa",
        "base_dim": 612,
        "bi_dropout": 0.5,
        "class_prediction_layer": {
            "activations": [
                "linear"
            ],
            "hidden_dims": [
                612
            ],
            "input_dim": 612,
            "num_layers": 1
        },
        "domain_slot_list_path": "./ontology/domain_slot_list_nosp.txt",
        "dropout": 0.5,
        "elmo_embedding_path": "./data/elmo_embeddings/",
        "initializer": [],
        "loss_scale_by_num_values": false,
        "phrase_layer": {
            "type": "gru",
            "bidirectional": true,
            "hidden_size": 306,
            "input_size": 800,
            "num_layers": 1
        },
        "span_end_encoder": {
            "activations": "relu",
            "hidden_dims": 612,
            "input_dim": 612,
            "num_layers": 1
        },
        "span_label_predictor": {
            "activations": [
                "relu",
                "linear"
            ],
            "hidden_dims": [
                612,
                3
            ],
            "input_dim": 612,
            "num_layers": 2
        },
        "span_prediction_layer": {
            "activations": [
                "linear"
            ],
            "hidden_dims": [
                612
            ],
            "input_dim": 612,
            "num_layers": 1
        },
        "span_start_encoder": {
            "activations": "relu",
            "hidden_dims": 612,
            "input_dim": 612,
            "num_layers": 1
        },
        "symbol_embedder": {
            "symbols": {
                "type": "embedding",
                "embedding_dim": 128,
                "num_embeddings": 40
            }
        },
        "text_field_char_embedder": {
            "token_characters": {
                "type": "character_encoding",
                "dropout": 0,
                "embedding": {
                    "embedding_dim": 20,
                    "num_embeddings": 262
                },
                "encoder": {
                    "type": "cnn",
                    "embedding_dim": 20,
                    "ngram_filter_sizes": [
                        5
                    ],
                    "num_filters": 100
                }
            }
        },
        "text_field_embedder": {
            "allow_unmatched_keys": true,
            "elmo": {
                "type": "elmo_token_embedder",
                "do_layer_norm": false,
                "dropout": 0,
                "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x2048_256_2048cnn_1xhighway/elmo_2x2048_256_2048cnn_1xhighway_options.json",
                "requires_grad": false,
                "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x2048_256_2048cnn_1xhighway/elmo_2x2048_256_2048cnn_1xhighway_weights.hdf5"
            }
        },
        "token_indexers": {
            "elmo": {
                "type": "elmo_characters"
            },
            "token_characters": {
                "type": "characters",
                "character_tokenizer": {
                    "byte_encoding": "utf-8",
                    "end_tokens": [
                        260
                    ],
                    "start_tokens": [
                        259
                    ]
                },
                "min_padding_length": 5
            }
        },
        "use_graph": true,
        "use_pre_calc_elmo_embeddings": true,
        "word_embeddings": "elmo"
    },
    "train_data_path": "./data/train.json",
    "validation_data_path": "./data/dev.json",
    "trainer": {
        "cuda_device": -1,
        "keep_serialized_model_every_num_seconds": 1800,
        "num_epochs": 5,
        "num_serialized_models_to_keep": 1,
        "optimizer": {
            "type": "adam",
            "lr": 0.001
        }
    },
    "vocabulary": {
        "min_count": {
            "tokens": 0
        }
    },
    "validation_iterator": {
        "type": "bucket",
        "batch_size": 36,
        "max_instances_in_memory": 1000,
        "sorting_keys": [
            [
                "dialogs",
                "num_tokens"
            ]
        ],
        "track_epoch": true
    }
}